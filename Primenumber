def prime(num):

    #num=int(input("Enter a number please : "))
    if num==1:
        print("This is a prime number")
    for i in range(2,num):
        if num%i==0:
            print("This is not a prime number")
            break
        else:
            print("This is a prime number")
            break


prime(5)
prime(7)
prime(9)
prime(10)
